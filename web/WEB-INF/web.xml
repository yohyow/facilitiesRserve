<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>edu.facilities.servlet.view.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Application</servlet-name>
        <servlet-class>edu.facilities.servlet.view.Application</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserManager</servlet-name>
        <servlet-class>edu.facilities.servlet.view.UserManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>gradeManager</servlet-name>
        <servlet-class>edu.facilities.servlet.view.gradeManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>baseManager</servlet-name>
        <servlet-class>edu.facilities.servlet.view.BaseServletManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FacilitiesManager</servlet-name>
        <servlet-class>edu.facilities.servlet.view.FacilitiesManager</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReserveManager</servlet-name>
